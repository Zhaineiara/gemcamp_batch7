<br>
<table class="table table-info table-striped">
  <thead class="table-dark">
  <tr>
    <th scope="col">#</th>
    <th scope="col">Name</th>
    <th scope="col">Email</th>
    <th scope="col">Phone Number</th>
  </tr>
  </thead>
  <tbody>

  <% @users.each do |user| %>
    <tr>
      <td><%= user.id %></td>
      <td><%= user.name %></td>
      <td><%= user.email %></td>
      <td><%= user.phone_number %></td>
    </tr>
  <% end %>

  </tbody>
</table>